                    <% } else { %>
                        <li><a href="/login">Login</a></li>
                    <% } %>
                    <div class="navbar-toggler">
                        <i style="font-size: x-large;"class="fas fa-bars"></i> <!-- Hamburger icon -->
                    </div>
                </ul>
            </div>
                <div class="sidebar" id="sidebar">
